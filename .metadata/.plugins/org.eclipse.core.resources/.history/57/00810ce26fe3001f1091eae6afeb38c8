	myJsonObject8 =
	{
		"test" :
		[
			"ccc", "ddd"
		]
		, "exam" :
		[
			"kkk", "ppp"
		]
			  
	}
	
	/* myJsonObject8 = {"test":["ccc", "ddd"], "exam" :["kkk", "ppp"]} */

</script>

